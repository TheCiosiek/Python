def convert_string_to_number (str):
    str=int(str)
    print (str)
    return

def print_list(list):
    for i in list:
        print(i)
    return

def find_min(values):
    int_values=[]
    values=values.split()
    for i in values:
        int_values.append(int(i))
    x=int_values[0]
    for i in int_values:
        if x>i:
            x=i
    return print(f"Najmniejsza wartość to: {x}")

def print_dict(dict):
    for key in dict_to_print:
        print(f"„{key}” {dict_to_print[key]}")
    return

def get_length(string):
    x=len(string)
    print(f"Twoje słowo ma {x} znaków")
    return x

def remove_outer_characters(string, len):
    string=string[1:len-1]
    print(f"Nowe słowo to {string}")
    return

value=input("Wprowadź wartość: ")
convert_string_to_number(value)

list=[1,2,3,4,5]
print_list(list)

dict_to_print={"a":1,"b":2,"c":3}
print_dict(dict_to_print)

values=input("Wprowadź wartości oddzielone spacją, a znajdę najmniejszą z tych liczb\n")
find_min(values)

string=input("Wprowadź słowo, oblicze jego długość i skrócę o pierwszy i ostatni znak: ")
len_string=get_length(string)
remove_outer_characters(string, len_string)

