def convert_string_to_number (str):
    str=int(str)
    print (str)
    return

def print_list(list):
    for i in list:
        print(i)
    return

def find_min(values):
    int_values=[]
    values=values.split()
    for i in values:
        int_values.append(int(i))
    x=int_values[0]
    for i in int_values:
        if x>i:
            x=i
    return print(f"Najmniejsza wartość to: {x}")

def print_dict(dict):
